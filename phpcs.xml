<?xml version="1.0"?>
<ruleset name="Supplemental for php-cs-fixer">
  <config name="installed_paths" value="./vendor/slevomat/coding-standard"/>
  <rule ref="PSR12"/>
  <exclude-pattern>*/cache/*</exclude-pattern>
  <rule ref="SlevomatCodingStandard.Files.SideEffectFoundSniff"/>
  <rule ref="SlevomatCodingStandard.Commenting.DocCommentSpacingSniff"/>
  <rule ref="SlevomatCodingStandard.Namespaces.UnusedUsesSniff"/>
  <rule ref="Generic.CodeAnalysis.EmptyStatement"/>
</ruleset>
